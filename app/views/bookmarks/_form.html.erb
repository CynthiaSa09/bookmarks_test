<%= form_with(model: bookmark, remote: true) do |form| %>
  <% if bookmark.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(bookmark.errors.count, "error") %> prohibited this bookmark from being saved:</h2>

      <ul>
        <% bookmark.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div class="field">
    <%= form.label :url %>
    <%= form.text_field :url %>
  </div>

  <div class="field">
   <h5><%= form.label "Select the categories" %></h5>
   <%= form.collection_chech_boxes :category_ids, Category.all, :id, :name do |cat| %>
   <%= cat.label %>
   <%= cat.chech_box %>
  <%= end %>
  </div>

  <div class="field">
  <h5><%= form.label "Select the kinds" %></h5>
   <%= form.collection_chech_boxes :kinds_ids, Kind.all, :id, :name do |cat| %>
   <%= cat.label %>
   <%= cat.chech_box %>
  <%= end %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
